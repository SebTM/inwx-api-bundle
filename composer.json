{
    "name": "sebtm/inwx-api-bundle",
    "description": "A Symfony bundle for using \"inwx/php-client\".",
    "keywords": [ "inwx", "php", "client", "api", "symfony"],
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Sellmeier",
            "email": "mail@sebastian-sellmeier.de"
        }
    ],
    "require": {
        "php": ">=7.2",
        "inwx/domrobot": "^3.0",
        "symfony/config": "^3.4|^4.1|^4.2",
        "symfony/http-kernel": "^3.4|^4.1|^4.2",
        "symfony/yaml": "^3.4|^4.1|^4.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2",
        "matthiasnoback/symfony-dependency-injection-test": "~4",
        "php-coveralls/php-coveralls": "~2",
        "phpunit/phpunit": "~8"
    },
    "autoload": {
        "psr-4": {
            "SebTM\\INWX\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SebTM\\INWX\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
