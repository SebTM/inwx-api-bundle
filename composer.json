{
    "name": "sebtm/inwx-api-bundle",
    "description": "A Symfony bundle for using \"inwx/php-client\".",
    "keywords": [ "inwx", "php", "client", "api", "symfony"],
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Sellmeier",
            "email": "mail@sebastian-sellmeier.de"
        }
    ],
    "require": {
        "php": ">=5.6",
        "inwx/domrobot": "^2.4",
        "symfony/config": "^2.8|^3.4|^4.1"
    },
    "require-dev": {
        "symfony/framework-bundle": "^2.8|^3.4|^4.1",
        "symfony/yaml": "^2.8|^3.4|^4.0|^4.1",
        "phpunit/phpunit": "^5.7|^6.5|^7.0"
    },
    "autoload": {
        "psr-4": {
            "SebTM\\INWX\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SebTM\\INWX\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
